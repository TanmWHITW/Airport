<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Airport">

    <FontFamily x:Key="MontserratRegular">pack:://application;,,,/Fonts/#Montserrat Regular</FontFamily>
    <FontFamily x:Key="MontserratRegularItalic">pack:://application;,,,/Fonts/#Montserrat Italic</FontFamily>

    <FontFamily x:Key="MontserratThin">pack:://application;,,,/Fonts/#Montserrat Thin</FontFamily>
    <FontFamily x:Key="MontserratThinItalic">pack:://application;,,,/Fonts/#Montserrat Thin Italic</FontFamily>

    <FontFamily x:Key="MontserratBold">pack:://application;,,,/Fonts/#Montserrat Bold</FontFamily>
    <FontFamily x:Key="MontserratBoldItalic">pack:://application;,,,/Fonts/#Montserrat Bold Italic</FontFamily>

</ResourceDictionary>